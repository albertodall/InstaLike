{
  "AppSettings": {
    "DeploymentType": "OnPrem"
  },
  "LogSettings": {
    "LogFile": "InstaLike.log",
    "FlushToDiskIntervalSeconds": 15
  },
  "AppInsights": {
    "AppInsightsInstrumentationKey": ""
  },
  "ConnectionStrings": {
    "DefaultDatabase": "Data Source=(local); Initial Catalog=InstaLike; Integrated Security=SSPI; Application Name=InstaLike.Web"
  },
  "ExternalStorage": {
    "AzureBlobStorage": {
      "StorageConnectionString": "UseDevelopmentStorage=true"
    }
  }
}
